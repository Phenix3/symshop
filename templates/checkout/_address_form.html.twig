
<div class="two fields">
    {{ form_row(form.name) }}
    {{ form_row(form.firstName) }}
</div>
<div class="two fields">
    {{ form_row(form.civility, {expanded: false}) }}
    {{ form_row(form.company) }}
</div>
<div class="two fields">
    {{ form_row(form.address) }}
    {{ form_row(form.addressbis) }}
</div>
<div class="two fields">
    {{ form_row(form.bp) }}
    {{ form_row(form.postal) }}
</div>
<div class="two fields">
    {{ form_row(form.city) }}
    {{ form_row(form.country) }}
</div>
<div class="two fields">
    {{ form_row(form.phone) }}
    {# {{ form_row(form.isProfessionnal) }} #}
    <div class="field">
        <div class="ui toggle checkbox">
            {{ form_widget(form.isProfessionnal) }}
            {{ form_label(form.isProfessionnal) }}
        </div>
    </div>
</div>
